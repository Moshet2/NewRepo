<!DOCTYPE html>

<html>
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=Edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/jquery-ui.css" rel="stylesheet" />
    <link href="~/Content/Site.css" rel="stylesheet" />
    @*@Styles.Render("~/Content/css")*@
</head>
<body>
    <div class="nav navbar-btn navColor">
        <div class="navbar-collapse collapse ">
            <ul class="nav navbar-nav">
                <li>@Html.ActionLink("Currency Converter", "CurrencyConverter", "Home")</li> 
                <li>@Html.ActionLink("About", "About", "Home")</li>
            </ul>
        </div>
    </div>
     
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer class="direct">
            <p>
                @Html.ActionLink("Download Help File", "DownloadFile", new { fileName = 1 })
            </p>
            <p>
                <a href="#" id="History">History</a>
            </p>

            <div class="text-center center-block">
                <div class="text-center equaletxt" id="HistoryView"></div>
            </div>
        </footer>
    </div>
    @*<div>
            @RenderBody()
        </div>*@
    @*@Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/bundles/bootstrap")*@
    <script src="~/Scripts/jquery.3.3.1.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Scripts/JavaScript.js"></script>
</body>
</html>
